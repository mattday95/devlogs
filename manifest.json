{
    "name": "Dev Logs",
    "description": "Keep track of important dev resources and code snippets",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/assets/dist/images/get_started16.png",
            "32": "/assets/dist/images/get_started32.png",
            "48": "/assets/dist/images/get_started48.png",
            "128": "/assets/dist/images/get_started128.png"
        }
    },
    "icons": {
        "16": "/assets/dist/images/get_started16.png",
        "32": "/assets/dist/images/get_started32.png",
        "48": "/assets/dist/images/get_started48.png",
        "128": "/assets/dist/images/get_started128.png"
    },
    "permissions": ["storage", "tabs", "webNavigation", "notifications"]
  }